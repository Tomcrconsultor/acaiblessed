{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "csa/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/csa/(.*)",
      "dest": "/csa/$1"
    },
    {
      "src": "/csa",
      "dest": "/csa/index.html"
    }
  ]
} 